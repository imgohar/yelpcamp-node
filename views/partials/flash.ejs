<% if(success != "") { %>
<div class="alert alert-success alert-dismissible fade show" role="alert">
    <strong>New Message!</strong> <%= success %>
    <button
        type="button"
        class="btn-close"
        data-bs-dismiss="alert"
        aria-label="Close"
    ></button>
</div>

<% } %> <% if(error != "") { %>
<div class="alert alert-danger alert-dismissible fade show" role="alert">
    <strong>New Message!</strong> <%= error %>
    <button
        type="button"
        class="btn-close"
        data-bs-dismiss="alert"
        aria-label="Close"
    ></button>
</div>

<% } %>
